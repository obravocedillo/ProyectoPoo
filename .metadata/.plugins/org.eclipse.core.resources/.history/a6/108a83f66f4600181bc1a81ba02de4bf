
public class GameMotor implements Runnable {
	static double FPS = 30.0;
	
	public GameMotor(){
		Thread thread = new Thread(this);
		thread.start();
	}

	@Override
	public void run() {
		try {
			System.out.println(FPS);
			Thread.sleep(50);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
		
		
		
	}
	
	
	
	public static void main(String[] args){
			GameMotor gm = GameMotor();
	}
	
}
